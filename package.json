{
  "name": "kittenTricks",
  "version": "2.0.0",
  "private": true,
  "author": "akveo",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:dev": "npm run env:set -- dev && npm start -- -c",
    "start:prod": "npm run env:set -- prod && npm start -- -c",
    "env:set": "./scripts/environment/set-env.sh",
    "build": "tsc -p ./tsconfig.json",
    "clean": "rimraf ./dist",
    "lint": "tslint -p ./tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "ci:install": "npm ci",
    "ci:lint": "npm run lint",
    "ci:build": "npm run env:set -- prod && npm run build"
  },
  "dependencies": {
    "@eva-design/eva": "1.0.0-beta.2",
    "expo": "^33.0.4",
    "expo-analytics": "^1.0.8",
    "react": "^16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-ui-kitten": "github:akveo/react-native-ui-kitten#next",
    "react-navigation": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/runtime": "^7.1.2",
    "@types/react": "^16.8.19",
    "@types/react-native": "^0.57.60",
    "@types/react-navigation": "^3.0.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "husky": "^1.1.2",
    "rimraf": "^2.6.2",
    "tslint": "^5.12.1",
    "typescript": "^3.5.1"
  }
}
